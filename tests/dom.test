#    dom-6.*:  appendChild
#    dom-7.*:  getElementsByTagName
#    dom-8.*:  getElementsByTagNameNS
#    dom-9.*:  nodeValue  
#    dom-10.*: setAttribute, again setAttributeNS
#    dom-11.*: disableOutputEscaping 
