    set doc [dom parse {<r><z/><e><a/>text<b><c/></b></e></r>}]
    set root [$doc documentElement]
    set removedNode [$root removeChild [$root firstChild]]
    $root appendChild [[$root firstChild] cloneNode -deep]
    set result [$root asXML -indent none]
    $doc delete
